{
  "name": "@zk-id/example-web-app",
  "version": "0.6.1",
  "description": "Web application example demonstrating zk-id integration",
  "private": true,
  "scripts": {
    "setup:deps": "npm run build --workspace=@zk-id/core && npm run build --workspace=@zk-id/sdk && npm run build --workspace=@zk-id/issuer",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:dev": "ts-node src/server.ts",
    "predev": "npm run setup:deps",
    "dev": "nodemon --exec ts-node src/server.ts",
    "test": "echo 'No tests for examples'"
  },
  "dependencies": {
    "@zk-id/core": "*",
    "@zk-id/issuer": "*",
    "@zk-id/sdk": "*",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.2",
    "mocha": "^12.0.0-beta-8",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.9.3"
  }
}
